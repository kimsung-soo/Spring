package com.yedam;

import org.junit.jupiter.api.Test;

public class CarTest {
	@DisplayName 
	@Test	
	public void test() {
		Car car1 = new Car("베뉴"); //객체 선언	
		Car car2 = Car.getInstance("Qm6"); //객체 선언	
		
		car1.drive();		
		car2.drive();
		
		assertEquals (car1,car2);
	}
	@Test	
	public void test() {
		Car car1 = Car.getInstance("베뉴"); //객체 선언	
		Car car2 = Car.getInstance("Qm6"); //객체 선언	
		
		car1.drive();		
		car2.drive();
		
		assertEquals (car1,car2);
	}
	
	
}


